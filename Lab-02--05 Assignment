      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "budget = pd.DataFrame({\n",
        "    'Dept': ['HR','Finance','IT','Marketing','Sales'],\n",
        "    'Budget': [50000,70000,120000,90000,150000]\n",
        "})\n",
        "expense = pd.DataFrame({\n",
        "    'Dept': ['HR','Finance','IT','Marketing','Sales'],\n",
        "    'Expense': [52000,68000,130000,95000,140000]\n",
        "})\n",
        "df = pd.merge(budget, expense, on='Dept')\n",
        "df['Difference'] = df['Expense'] - df['Budget']\n",
        "df['Over_Budget'] = df['Difference'] > 0\n",
        "print(df)\n",
        "df.plot(x='Dept', y=['Budget','Expense'], kind='bar')\n",
        "plt.show()"
      ]
